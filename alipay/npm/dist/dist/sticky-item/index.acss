.sticky-item-index {
    display: block;
    height: initial;
}
.sticky-item-index .i-sticky-item-header {
    background: #eee;
    font-size: 14px;
    width: 100%;
    height: 32px;
    line-height: 32px;
}

.sticky-item-index .i-sticky-item-content {
    font-size: 14px;
}

.sticky-item-index .i-sticky-title {
    width: 100%;
    padding: 0 15px;
    box-sizing: border-box;
    background: #eee;
}

.sticky-item-index .i-sticky-fixed .i-sticky-title {
    position: fixed;
    top: 0;
}
